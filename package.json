{
  "name": "project-base-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node usage.js",
    "predev": "node package-info.js dev",
    "dev": "ng serve",
    "premock": "node package-info.js mock",
    "mock": "ng serve -c mock",
    "presit": "node package-info.js sit",
    "sit": "ng serve -c sit",
    "preqa": "node package-info.js qa",
    "qa": "ng serve -c qa",
    "homolog": "ng serve -c homolog",
    "preprod": "node package-info.js prod",
    "prod": "ng serve -c prod",
    "prebuild": "node package-info.js prod",
    "build": "ng build -c=prod",
    "test": "ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "test-chrome": "ng test --code-coverage --watch=true --browsers=Chrome",
    "lint": "ng lint",
    "webdriver-update": "webdriver-manager update",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}